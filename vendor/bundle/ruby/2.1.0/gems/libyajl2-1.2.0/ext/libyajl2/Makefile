TARGET = libyajl
DLLIB = $(TARGET).bundle
all:

install:
	mkdir -p /Users/hu/conoha-bootstrap/vendor/bundle/ruby/2.1.0/gems/libyajl2-1.2.0/lib/libyajl2/vendored-libyajl2/lib
	cp $(DLLIB) /Users/hu/conoha-bootstrap/vendor/bundle/ruby/2.1.0/gems/libyajl2-1.2.0/lib/libyajl2/vendored-libyajl2/lib/$(DLLIB)
	mkdir -p /Users/hu/conoha-bootstrap/vendor/bundle/ruby/2.1.0/gems/libyajl2-1.2.0/lib/libyajl2/vendored-libyajl2/include/yajl
	cp yajl/*.h /Users/hu/conoha-bootstrap/vendor/bundle/ruby/2.1.0/gems/libyajl2-1.2.0/lib/libyajl2/vendored-libyajl2/include/yajl
